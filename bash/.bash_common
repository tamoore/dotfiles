#!/usr/bin/env bash

unset MAILCHECK

BASH_IT="$HOME/.bash_it"
BASH_IT_THEME='bobby'
GIT_HOSTING='git@github.com'
TODO="t"
SCM_CHECK=false

export BASH_IT
export BASH_IT_THEME
export GIT_HOSTING
export TODO
export SCM_CHECK

# if running bash
if [[ -n "$BASH_VERSION" ]]; then
    # include .bashrc if it exists
    if [[ -f "$HOME/.bashrc" ]]; then
        # shellcheck source=/dev/null
        . "$HOME/.bashrc"
    fi
fi
